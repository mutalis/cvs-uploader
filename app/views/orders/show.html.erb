<p id="notice"><%= notice %></p>

<h1>Patients with active Invoices</h1>

<table>
  <thead>
    <tr>
      <th>Patient name</th>
      <th>Patient state</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @patients.each do |patient| %>
      <tr>
        <td><%= patient.patient_name %></td>
        <td><%= patient.state %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Active Orders', root_path %>
